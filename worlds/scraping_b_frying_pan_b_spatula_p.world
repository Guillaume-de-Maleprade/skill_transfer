<?xml version='1.0'?>
<sdf version="1.6">
    <world name="b_frying_pan_b_spatula_v">

        <include>
            <uri>model://sun</uri>
        </include>

        <include>
            <uri>model://ground_plane</uri>
        </include>

        <include>
            <uri>model://b_spatula</uri>
