<launch>
  <node pkg="skill_transfer" type="constraint_controller_robot" name="constraint_controller" output="screen"  unless="$(arg sim)"/>
  <node pkg="skill_transfer" type="constraint_controller_simulator" name="constraint_controller" output="screen"  if="$(arg sim)"/>
  
  <node pkg="skill_transfer" type="task_executive" name="task_executive"
        output="screen">
    <param name="task_file" type="string" value="$(find skill_transfer)/tasks/scraping_butter.yaml" />

    <param name="motion_directory" type="string" value="$(find skill_transfer)/motions/" unless="$(arg sim)" />
    <param name="motion_directory" type="string" value="$(find skill_transfer)/motions/simulator/" if="$(arg sim)" />
  </node>
<!--  <node pkg="tf" type="static_transform_publisher" name="map_to_world" args="0 0 0 0 0 0 world base_link 100"/>-->
</launch>
